		<inclure>abomailmans_interface.php</inclure>
	</pipeline>

	<pipeline>
		<nom>header_prive</nom>
		<inclure>abomailmans_interface.php</inclure>
	</pipeline>

	<pipeline>
		<nom>insert_head</nom>
