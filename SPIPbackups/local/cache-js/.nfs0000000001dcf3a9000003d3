/* #PRODUIRE{fond=googleplus1.js} le 2012-03-11 17:00:25 */
// googleplus1.js


(function($) {
	var googleplus1_init = function() {
		var selector = $('nada');
		if (!selector.length) return;

		var ul = $('<ul><\/ul>');
		
		selector.after(ul.wrap('<div class="googleplus1"><\/div>').parent());

		$('<li><g:plusone size="medium"></g:plusone></li>').appendTo(ul);


	};
	$(function(){
		$(googleplus1_init);
	});
})(jQuery);
